{
  "name": "@tajwal/build-ai-agent",
  "version": "1.0.0-alpha.8",
  "description": "Framework-agnostic SDK for building AI agents",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.js"
    },
    "./tools": {
      "types": "./dist/tools/index.d.ts",
      "import": "./dist/tools/index.mjs",
      "require": "./dist/tools/index.js"
    },
    "./flows": {
      "types": "./dist/flows/index.d.ts",
      "import": "./dist/flows/index.mjs",
      "require": "./dist/flows/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agents",
    "llm",
    "openai",
    "chatbot",
    "workflow",
    "automation",
    "framework-agnostic",
    "tajwal",
    "build-ai-agent"
  ],
  "author": "Tajwal Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LinuxDevil/agent-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/LinuxDevil/agent-sdk/issues"
  },
  "homepage": "https://github.com/LinuxDevil/agent-sdk#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@ai-sdk/openai": "^0.0.42",
    "ai": "^4.3.19",
    "ollama-ai-provider": "^1.2.0",
    "zod": "^3.25.76"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/openai": {
      "optional": true
    },
    "ollama-ai-provider": {
      "optional": true
    }
  },
  "dependencies": {
    "dedent": "^1.5.3",
    "dotenv": "^17.2.3",
    "nanoid": "^5.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@vitest/coverage-v8": "^1.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3",
    "vitest": "^1.0.0"
  }
}

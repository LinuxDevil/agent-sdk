{
  "name": "@tajwal/build-ai-agent",
  "version": "1.0.0-alpha.8",
  "description": "Framework-agnostic SDK for building AI agents",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.js"
    },
    "./tools": {
      "types": "./dist/tools/index.d.ts",
      "import": "./dist/tools/index.mjs",
      "require": "./dist/tools/index.js"
    },
    "./flows": {
      "types": "./dist/flows/index.d.ts",
      "import": "./dist/flows/index.mjs",
      "require": "./dist/flows/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "agents",
    "llm",
    "openai",
    "chatbot",
    "workflow",
    "automation",
    "framework-agnostic",
    "tajwal",
    "build-ai-agent"
  ],
  "author": "Tajwal Team",
  "license": "MIT",
  "peerDependencies": {
    "ai": "^4.1.54",
    "zod": "^3.23.8",
    "@ai-sdk/openai": "^0.0.42",
    "ollama-ai-provider": "^1.2.0"
  },
  "dependencies": {
    "nanoid": "^5.0.7",
    "dedent": "^1.5.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "tsup": "^8.0.0",
    "typescript": "^5",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "directory": "src/packages/sdk"
  }
}
